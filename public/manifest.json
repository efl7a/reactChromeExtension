{
  "name": "Motivational Extension",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "48": "dog48.png",
    "128": "dog128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
   "permissions": ["tabs", "storage"],
   "browser_action": {
     "default_popup": "index.html?query=popup",
     "default_icons": {
       "48": "dog48.png",
       "128": "dog128.png"
     }
   }
}
